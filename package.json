{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "husky",
    "db:generate": "drizzle-kit generate"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.33",
    "@iconify-json/simple-icons": "^1.2.29",
    "@nuxt/image": "^1.10.0",
    "@nuxt/ui": "3.0.0",
    "@nuxthub/core": "0.8.18",
    "drizzle-orm": "^0.40.1",
    "nuxt": "^3.16.1",
    "nuxt-auth-utils": "0.5.17"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@nuxt/eslint": "^1.2.0",
    "@types/bun": "^1.2.8",
    "drizzle-kit": "^0.30.6",
    "eslint": "^9.23.0",
    "eslint-plugin-format": "^1.0.1",
    "husky": "^9.1.7",
    "typescript": "^5.8.2",
    "vue-tsc": "2.2.2",
    "wrangler": "^4.6.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "*"
      ],
      "allowAny": [
        "*"
      ]
    },
    "patchedDependencies": {
      "@nuxt/image": "patches/@nuxt__image.patch"
    }
  }
}
